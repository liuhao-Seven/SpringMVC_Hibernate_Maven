.component-panel-content{
	display: none;
	position: absolute;
	bottom: 0px;
	z-index: 2000;
	width: 100%;
	background-color: white;
	height: 15.5em;
	overflow-y: auto;
	text-align: center;
}

.component-panel-item{
	width: 90%;
	margin:auto;
	border-width:0 0 1px 0;
	border-bottom-color: #DFDFDF;
	border-bottom-style: solid;
	background-color: white;
	padding: 0.5em;
	text-wrap:unrestricted;
}
.component-panel-item:ACTIVE {
	background-color: #DFDFDF;
}
.component-panel-btn{
	background-color: black;
	height:3em;
	line-height:3em;
	color: white;
	text-shadow:none;
	font-weight: bold;
}

.component-panel-btn-left{
	float:left;
	padding-left:1.5em;
	width: 20%;
	text-align: left;
}

.component-panel-btn-right{
	float:right;
	padding-right:1.5em;
	width: 20%;
	text-align: right;
}

.button-ui{
	position: fixed;
	bottom: 0px;
	text-align: center;
	width: 100%;
	border: 1px #DEDEDE solid;
	color:white;
	padding: 0.5em 0 0.5em 0;
	background-color: red;
}
.component-messagebox-win{
	width:80.3%;
	position:fixed;
	top:40%;
	margin-top:-4.15em;
	left:50%;
	margin-left:-40.15%;
	background-color:white;
	border-radius:5px;
	z-index:1000;
}
.component-messagebox-title{
	font-size:1rem;
	width:100%;
	text-align:center;
	color:#000;
	font-family:Hiragino Sans GB W3;
}
.component-messagebox-msg{
	padding-top:1.5em;
	padding-bottom:1.5em;
	padding-left:0.3em;
	padding-right:0.3em;
	width:95%;
	vertical-align:middle;
	margin-top:0.375em;
	font-size:0.875rem;
	color:#353535;
	font-family:'Helvetica';
	text-align:center;
	font-weight:bold;
	word-wrap:break-word;
	word-break:break-all;
}
.component-messagebox-btnContainer{
	border-top :1px solid #aaa;
	display:-webkit-box;
	-webkit-flow:row;	
}
.component-messagebox-btn{
	text-align:center;
	color:#64ADFB;
	height:2.8em;
	line-height:2.8em;
	font-size:1rem;
	-webkit-box-flex:1;
	font-family: "Hiragino Sans GB W3";
}
.component-messagebox-btn:nth-child(n+2){
	border-left: 1px solid #aaa;
}

.component-messagebox-btn:HOVER{
	background-color: #ccc;
}



