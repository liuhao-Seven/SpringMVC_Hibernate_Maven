header{
	width: 100%;
	background: url(../../images/3.4.png) no-repeat ;
	background-size: 100% ;
	margin-bottom: 1rem;
}
.tax{
	height: 5rem;
	margin-top: .5rem;
}
.tax-left{
	width: 25%;
	float: left;
	max-width: 250px;
}
.tax-right{
	width: 70%;
	float: left;
	margin-top: 1rem;
}
.tax-right span{
	display: block;
	margin-left: 1rem;
	margin-top: 0.5rem;
}
.tax-right-health{
	color: #718900;
	font-size: 1.0625rem;
}
.tax-right-txt{
	color: #353535;
	font-size: 0.75rem;
}

.maintittle .left{
	background-color: #94B204;
	height: 3rem;
	border: 3px solid #999999;
	border-radius: 0.5rem;
	width: 90%;
}

.maintittle{
	margin-bottom: 1rem;
}

.maintittle h3{
	position: relative;
	top: -0.3rem;
	left: 5%;
}

#tittledet{
	color: white;
}

#num{
	text-align: center;
	border: 3px solid #999999;
	border-radius: 0.5rem;
	width: 2rem;
	height: 2rem;
	background-color: white;
	color: #94B204;
	float: left;
	margin-left:0.3rem ;
	margin-top: 0.3rem;
}

#num h2{
	position: relative;
	top: -1rem;
}

.right{
	z-index: 1;
	position: relative;
	left: 90%;
	border: 3px solid #999999;
	width: 4rem;
	height: 4rem;
	background-color: white;
	border-radius: 3rem;
	text-align: center;
	top: -4.4rem;
}

.right h1{
	position: relative;
	top: -1.2rem;
	font-size: 2.5rem;
	font-family: "方正大黑简体";
	font-weight: 800;
	color: #94B204;
}




