package com.mvc.dto;


 /**
 * 【类或接口功能描述】
 * 
 * @version 1.0
 * @since 2015-2-10 下午5:01:24  
 * @author ChenYing
 */
public class MessageDto {
    
    private String code = "0";//代码：0-失败；1-成功
    private String message = "操作失败";//消息
    private Object obj;
    
    public String getCode() {
        return code;
    }
    public void setCode(String code) {
        this.code = code;
    }
    public String getMessage() {
        return message;
    }
    public void setMessage(String message) {
        this.message = message;
    }
    public Object getObj() {
        return obj;
    }
    public void setObj(Object obj) {
        this.obj = obj;
    }
    @Override
    public String toString() {
        return "MessageDto [code=" + code + ", message=" + message + ", obj=" + obj + "]";
    }
    
}
